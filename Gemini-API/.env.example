# Gemini API Configuration
# Copy this file to .env and fill in your values

# API Key for authentication (Bearer token)
GEMINI_API_KEY=sk-demo

# Google Gemini Cookies (required)
# Get these from https://gemini.google.com → F12 → Application → Cookies
GEMINI_COOKIE_1PSID=YOUR_SECURE_1PSID_HERE
GEMINI_COOKIE_1PSIDTS=YOUR_SECURE_1PSIDTS_HERE

# Server Configuration
GEMINI_HOST=0.0.0.0
GEMINI_PORT=8989

# Model Configuration
GEMINI_DEFAULT_MODEL=gemini-2.5-flash

# Timeout in seconds
GEMINI_TIMEOUT=120
